<wfs:Transaction service="WFS" version="1.1.0"
  xmlns:wfs="http://www.opengis.net/wfs"
  xmlns:gml="http://www.opengis.net/gml">
  <wfs:Insert>
    <{{ layer_name }}>
    <the_geom>
      <Point srsName="http://www.opengis.net/gml/srs/epsg.xml#4326">
        <pos>{{ coords }}</pos>
      </Point>
    </the_geom>
    {{ property_element }}
    </{{ layer_name }}>
  </wfs:Insert>
</wfs:Transaction>
